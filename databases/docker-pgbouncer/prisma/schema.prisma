datasource db {
  provider = "postgresql"
  url      = "postgresql://this-will-not-be-used-but-overwritten-in-the-prisma-client-constructor"
}

generator client {
  provider = "prisma-client-js"
  engineType = "binary"
}

model User {
  email String @default("")
  id    String @id
  name  String @default("")
}
