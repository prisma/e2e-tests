generator client {
    provider = "prisma-client-js"
}

generator typegraphql {
    provider = "typegraphql-prisma"
    output   = "./generated/typegraphql-prisma"
}

datasource postgresql {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id    String   @id @default(cuid())
    email String   @unique
    name  String?
    type  UserType
}

enum UserType {
    ADMIN
    NORMAL
}
