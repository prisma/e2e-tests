
datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}
generator client {
  provider        = "prisma-client-js"
  
  
}
model User {
  id    Int    @id
  name  String
  email String
  posts Post[]
}

model Post {
  id     Int @id
  title  String
  User   User   @relation(fields: [userId], references: [id])
  userId Int
}