{
  "name": "nestjs-prisma-client-starter",
  "version": "0.0.1",
  "description": "NestJS Prisma Client Starter Project",
  "author": "Marc Stammerjohann",
  "license": "MIT",
  "keywords": [
    "NestJS",
    "Prisma",
    "Prisma Client",
    "Typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fivethree-team/nestjs-prisma-client-example.git"
  },
  "bugs": {
    "url": "https://github.com/fivethree-team/nestjs-prisma-client-example/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lift:save": "npx prisma2 migrate save --experimental",
    "lift:up": "npx prisma2 migrate up --experimental",
    "prisma:generate": "npx prisma2 generate",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "prisma2 migrate up --experimental && prisma2 generate && npm run seed",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "7.3.0",
    "@nestjs/core": "7.3.0",
    "@nestjs/graphql": "6.6.2",
    "@nestjs/jwt": "7.0.0",
    "@nestjs/passport": "7.1.0",
    "@nestjs/platform-express": "7.3.0",
    "@nestjs/swagger": "4.5.12",
    "@prisma/client": "2.2.0-dev.34",
    "apollo-server-express": "2.11.0",
    "bcrypt": "5.0.0",
    "class-transformer": "0.2.3",
    "class-validator": "0.11.1",
    "graphql": "14.6.0",
    "graphql-tools": "6.0.11",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5",
    "swagger-ui-express": "4.1.4",
    "type-graphql": "0.17.6"
  },
  "devDependencies": {
    "@nestjs/cli": "7.4.1",
    "@nestjs/testing": "7.3.0",
    "@prisma/cli": "2.2.0-dev.34",
    "@types/bcrypt": "3.0.0",
    "@types/chance": "1.1.0",
    "@types/express": "4.17.4",
    "@types/jest": "26.0.2",
    "@types/node": "12.12.47",
    "@types/supertest": "2.0.10",
    "chance": "1.1.6",
    "jest": "26.1.0",
    "prettier": "2.0.4",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "ts-jest": "26.1.1",
    "ts-loader": "7.0.5",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.2",
    "typescript": "3.9.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
