{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "postinstall": "export FIREBASE_FUNCTIONS_PG_URL=HAHA && yarn prisma2 generate"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-alpha.739",
    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.3.0"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.1.6",
    "prisma2": "^2.0.0-alpha.739",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "private": true
}
