{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  },
  "tracing": {
    "consoleLevel": "verbose"
  },
  "logger": {
    "categoryFilter": {
      "defaultLevel": "Trace"
    }
  },
  "logging": {
    "logLevel": {
      "default": "Trace",
      "Host.Results": "Trace",
      "Function": "Trace",
      "Host.Aggregator": "Trace"
    }
  }
}
