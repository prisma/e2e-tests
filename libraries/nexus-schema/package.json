{
  "name": "nexus-schema",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus-schema",
    "generate:prisma": "prisma generate",
    "generate:nexus-schema": "ts-node --transpile-only src/schema",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server"
  },
  "dependencies": {
    "@nexus/schema": "0.20.1",
    "@prisma/client": "2.14.0-dev.68",
    "@types/express": "4.17.8",
    "express": "4.17.1",
    "express-graphql": "0.12.0",
    "graphql": "15.4.0",
    "nexus-plugin-prisma": "0.27.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.14.0-dev.68",
    "@types/node": "12.19.12",
    "@types/react": "17.0.0",
    "@types/ws": "7.4.0",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.1",
    "typescript": "4.1.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
