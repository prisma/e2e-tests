{
  "name": "nexus-schema",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus-schema",
    "generate:prisma": "prisma generate",
    "generate:nexus-schema": "ts-node --transpile-only src/schema",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server"
  },
  "dependencies": {
    "@nexus/schema": "0.16.0",
    "@prisma/client": "2.10.0-dev.73",
    "@types/express": "4.17.8",
    "express": "4.17.1",
    "express-graphql": "0.11.0",
    "graphql": "15.3.0",
    "nexus-plugin-prisma": "0.22.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.10.0-dev.73",
    "@types/node": "12.19.2",
    "@types/react": "16.9.53",
    "@types/ws": "7.2.8",
    "ts-node": "9.0.0",
    "ts-node-dev": "1.0.0",
    "typescript": "4.0.5"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
