generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  // fake url to ensure the driver adapter is used
  url      = env("DRIVER_ADAPTERS_PLANETSCALE_NODE_BASIC_DATABASE_URL")
}

model User {
  id    String  @default(cuid()) @id
  email String  @unique
  name  String?
  age   Int?
}
