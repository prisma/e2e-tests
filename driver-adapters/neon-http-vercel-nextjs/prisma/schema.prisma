generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  // fake url to ensure the driver adapter is used
  // url      = "postgresql://foo"
  url      = env("DRIVER_ADAPTERS_NEON_HTTP_VERCEL_NEXTJS_DATABASE_URL")
}

model User {
  id    String  @id @default(cuid())
  email String  @unique
  name  String?
  age   Int?
}
