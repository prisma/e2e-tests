generator client {
  provider        = "prisma-client-js"
  binaryTargets   = ["native", "rhel-openssl-1.0.x"]
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  // fake url to ensure the driver adapter is used
  // url      = "postgresql://foo"
  url      = env("DRIVER_ADAPTERS_NEON_HTTP_LAMBDA_BASIC_DATABASE_URL")
}

model User {
  id    String  @id @default(cuid())
  email String  @unique
  name  String?
  age   Int?
}
