generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  // fake url to ensure the driver adapter is used
  url      = "mysql://prisma:prisma@localhost:3306/db"
}

model User {
  id    String  @default(cuid()) @id
  email String  @unique
  name  String?
  age   Int?
}
