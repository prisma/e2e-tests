generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  // fake url to ensure the driver adapter is used
  url      = env("DRIVER_ADAPTERS_NEON_HTTP_NODE_BASIC_DATABASE_URL")
}

model User {
  id    String  @id @default(cuid())
  email String  @unique
  name  String?
  age   Int?
}
