generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
  engineType      = "wasm"
  output =          "client"
}

datasource db {
  provider = "postgresql"
  // fake url to ensure the driver adapter is used
  url      = "postgresql://foo"
}

model User {
  id    String  @id @default(cuid())
  email String  @unique
  name  String?
  age   Int?
}
