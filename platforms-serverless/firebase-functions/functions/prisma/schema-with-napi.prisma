generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
  previewFeatures = ["nApi"]
}

datasource db {
  provider = "postgresql"
  url      = env("FIREBASE_FUNCTIONS_PG_URL")
}

model User {
  id    String  @id @default(cuid())
  email String  @unique
  name  String?
}
