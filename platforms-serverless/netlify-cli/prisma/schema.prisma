// Like, postgresql://user:password@localhost:5432/database/schema
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["rhel-openssl-1.0.x"]
  output        = "../functions/generated/client"
}

model User {
  id    String  @default(cuid()) @id
  email String  @unique
  name  String?
}
