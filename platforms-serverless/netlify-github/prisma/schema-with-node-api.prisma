generator client {
  provider      = "prisma-client-js"
  engineType    = "library"
  binaryTargets = "rhel-openssl-3.0.x"
}

datasource db {
  provider = "postgresql"
  url      = env("NETLIFY_BETA_PG_URL")
}

model User {
  id    String  @id @default(cuid())
  email String  @unique
  name  String?
}
